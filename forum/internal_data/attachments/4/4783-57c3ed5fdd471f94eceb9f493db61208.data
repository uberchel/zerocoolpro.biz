	}

	.horizontal-menu ul li a {
		background-color: #000000;
	}

	.horizontal-menu ul li a:hover {
		background-color: #212121;
	}

	::-webkit-scrollbar-thumb {
		background: #737373;
	}

	.filter-block {
		background-color: #454545;
	}

	.genre-block,
	.sort-block {
